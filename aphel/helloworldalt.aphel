@segment text
    @global main

main: mov str, I        # I is used as the pointer for the current character in the string

    loop:
        mov $I, A
        cmp A, '\0'
        jif 1, return
        mov A, 0xffff
        add I, 1
        jmp loop
        
    return:
        hcf

@segment rodata
    str: string "hello world!\0"