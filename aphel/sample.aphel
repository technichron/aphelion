@define asciistart, 'a'
@define asciiend, 'Z'

main:
    mov asciistart, a

    cmp a, asciiend
    jle loop

    invalidrange:
        out 'i'
        out 'n'
        out 'v'
        out 'a'
        out 'l'
        out 'i'
        out 'd'
        out ' '
        out 'r'
        out 'a'
        out 'n'
        out 'g'
        out 'e'
        out '!'
        out '\n'
        hcf

    loop:
        out a
        add a, 1
        cmp a, asciiend
        jmp loop
        hcf